{
  "name": "Anti Disturb",
  "description": "Blurs content on websites you may don't want to see",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ]
}